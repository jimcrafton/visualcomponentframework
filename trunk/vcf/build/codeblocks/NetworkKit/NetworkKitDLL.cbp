				</Linker>
				<ExtraCommands>
					<Add after="cmd /c if NOT EXIST ..\..\..\lib MKDIR ..\..\..\lib" />
					<Add after="cmd /c move ..\..\..\bin\libNetworkKit_mgcc.a ..\..\..\lib\libNetworkKit_mgcc.a" />
				</ExtraCommands>
				<Environment>
