					<Add library="odbc32" />
					<Add library="odbccp32" />
					<Add library="version" />
					<Add library="kernel32" />
					<Add library="user32" />
					<Add library="gdi32" />
					<Add library="winspool" />
					<Add library="comdlg32" />
					<Add library="comctl32" />
					<Add library="advapi32" />
					<Add library="shell32" />
					<Add library="ole32" />
					<Add library="oleaut32" />
					<Add library="uuid" />
					<Add library="ws2_32" />
					<Add library="opengl32" />
					<Add library="glu32" />
					<Add library="glaux" />
				</Linker>
