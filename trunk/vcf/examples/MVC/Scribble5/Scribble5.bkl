<?xml version="1.0" ?>

<makefile>
    <include file="../../../build/bakefiles/VCFCommon.bkl"/>

    <set var="CWD">$(VCF_DIR)/examples/MVC/Scribble5</set>

    <exe id="Scribble5" template="vcf_exe_gui" cond="OPTION_APPLICATIONKIT=='1'">
        <exename>$(id)</exename>
        <dirname>$(CWD)</dirname>
        <headers>
            $(CWD)/ScribbleController.h
            $(CWD)/ScribbleModel.h
            $(CWD)/ScribbleView.h
        </headers>
        <sources>$(CWD)/Scribble5.cpp</sources>
        <vcf-use-ApplicationKit/>
    </exe>
</makefile>

