<!--Autogenerated xmake makefile for project simple
-->


<config name="VC6 Debug">
	<!-- un-comment this out if you want to add
	custom include paths for this configuration.
		<include path=""/>
	-->

	<!-- compiler tool -->
	<tool id="cc" name="cl" type=".cc;.cpp;.cxx">
		<flags value="/nologo /MDd /W3 /Gm /GR /GX /ZI /Od /I .\ /DWIN32 /D_DEBUG /D_MBCS /FD /GZ /c /Fo$(dependency.output)"/>
		<exec command="cl /nologo /MDd /W3 /Gm /GR /GX /ZI /Od /I .\ /DWIN32 /D_DEBUG /D_MBCS /FD /GZ /Fo$(dependency.output) /c $(dependency.name) "/>
	</tool>

	<!-- linker tool -->
	<tool id="link" name="link" >
		<flags value="/nologo /subsystem:console /incremental:yes /pdb:$(target.output).pdb /debug /machine:I386 /out:$(target.output) /pdbtype:sept"/>
		<exec command="link /nologo /subsystem:console /incremental:yes /pdb:$(target.output).pdb /debug /machine:I386 /out:$(target.output) /pdbtype:sept $(dependsOn.children.output) "/>
	</tool>

	<!-- resource tool for compiling rc scripts-->
	<tool id="rc" name="rc.exe" type=".rc">
		<flags value="/v /d _DEBUG /d _AFXDLL /fo simple.res"/>
		<exec command="rc.exe /v /d _DEBUG /d _AFXDLL /fo simple.res $(dependency.name) "/>
	</tool>

</config>


<project name="simple" >
	<dependsOn>
		<!-- place a dependency entry here where the
		dependency is another project that must be built
		before this project. The form is:
			<dependency name="name_of_some_project"/>
		-->		
	</dependsOn>

	<!--put your project's targets here.-->
	<target name="simple.exe" output="Debug/simple.exe" tool="link" config="VC6 Debug">
		<dependsOn>			
			<dependency name="simple.cpp" output="Debug/simple.obj"/>
		</dependsOn>
	</target>
</project>

