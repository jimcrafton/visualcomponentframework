					<Add library="comctl32" />
					<Add library="ws2_32" />
					<Add library="opengl32" />
					<Add library="glu32" />
				</Linker>
				<ExtraCommands>
					<Add after="cmd /c if NOT EXIST ..\..\..\lib MKDIR ..\..\..\lib" />
					<Add after="cmd /c move ..\..\..\bin\libRemoteObjectKit_mgcc.a ..\..\..\lib\libRemoteObjectKit_mgcc.a" />
				</ExtraCommands>
			</Target>
