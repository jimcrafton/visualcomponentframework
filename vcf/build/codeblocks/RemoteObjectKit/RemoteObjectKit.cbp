				<Option type="3" />
				<Option compiler="gcc" />
				<Option includeInTargetAll="1" />
				<Option createStaticLib="1" />
				<Option projectLinkerOptionsRelation="2" />
				<Option projectIncludeDirsRelation="2" />
				<Option projectResourceIncludeDirsRelation="2" />
				<Option projectLibDirsRelation="2" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-w" />
					<Add option="-pipe" />
					<Add option="-frtti" />
					<Add option="-fexceptions" />
					<Add option="-mthreads" />
					<Add option="-fmessage-length=0" />
					<Add option="-Winvalid-pch" />
					<Add option="-Wno-multichar" />
					<Add option="-Wno-system-headers" />
					<Add option="-Wno-extra" />
					<Add option="-DNDEBUG" />
					<Add option="-DWIN32" />
					<Add option="-D_WIN32" />
					<Add option="-D_WINDOWS" />
					<Add option="-D_USRDLL" />
					<Add option="-DVCF_MINGW" />
					<Add option="-DVCF_DISABLE_PRAGMA_LINKING" />
					<Add option="-DUSE_NETKIT_DLL" />
					<Add option="-DREMOTEKIT_DLL" />
					<Add option="-DREMOTEKIT_EXPORTS" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="-Wl,--enable-runtime-pseudo-reloc" />
					<Add library="NetworkKit_$(COMPILER)$(COMPILER_VER)" />
					<Add library="FoundationKit_$(COMPILER)$(COMPILER_VER)" />
					<Add library="rpcrt4" />
					<Add library="odbc32" />
					<Add library="odbccp32" />
					<Add library="version" />
					<Add library="kernel32" />
					<Add library="user32" />
