<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="4" />
	<Project>
		<Option title="CreateBundle" />
		<Option compiler="gcc" />
		<Build>
			<Target title="mingw_Release">
				<Option output="..\..\bin\CreateBundle.exe" />
				<Option object_output="mgcc\Release\" />
				<Option external_deps="..\..\lib\libFoundationKit_mgcc.a;" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option use_console_runner="0" />
				<Option includeInTargetAll="1" />
				<Option projectResourceIncludeDirsRelation="2" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-W" />
					<Add option="-DNDEBUG" />
				</Compiler>
				<Linker>
					<Add library="libFoundationKit_mgcc.a" />
				</Linker>
			</Target>
			<Target title="mingw_Debug">
				<Option output="..\..\bin\CreateBundle.exe" />
				<Option object_output="mgcc\Debug\" />
				<Option external_deps="..\..\lib\libFoundationKit_mgcc_d.a;" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option use_console_runner="0" />
				<Option projectResourceIncludeDirsRelation="2" />
				<Compiler>
					<Add option="-W" />
					<Add option="-g" />
					<Add option="-D_DEBUG" />
				</Compiler>
				<Linker>
					<Add library="libFoundationKit_mgcc_d.a" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-pipe" />
			<Add option="-frtti" />
			<Add option="-fexceptions" />
			<Add option="-mthreads" />
			<Add option="-Wno-multichar" />
			<Add option="-DWIN32" />
			<Add option="-D_WIN32" />
			<Add option="-D_WINDOWS" />
			<Add option="-DVCF_MINGW" />
			<Add option="-DVCF_DISABLE_PRAGMA_LINKING" />
			<Add option="-DUSE_FOUNDATIONKIT_DLL" />
			<Add directory='&quot;$(VCF_INCLUDE)&quot;' />
		</Compiler>
		<Linker>
			<Add directory='&quot;$(VCF_LIB)&quot;' />
		</Linker>
		<Unit filename="CreateBundle.cpp">
			<Option compilerVar="CPP" />
			<Option target="mingw_Release" />
			<Option target="mingw_Debug" />
		</Unit>
	</Project>
</CodeBlocks_project_file>