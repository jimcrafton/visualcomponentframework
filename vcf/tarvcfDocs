echo Preparing list of files for vcf version $1...
find ./docs ./ -iname *.htm -o -iname *.html -o -iname *.jpg -o -iname *.jpeg -o -iname *.gif -o -iname *.css > docfiles.out
echo Creating documentation tar file...
tar -cf ./vcf_html_docs.$1.tar -T docfiles.out
tar -rf ./vcf_html_docs.$1.tar readme.txt
tar -rf ./vcf_html_docs.$1.tar vcfLicense.txt
rm docfiles.out
echo Compressing tar file with gzip...
gzip -f ./vcf_html_docs.$1.tar
mv ./vcf_html_docs.$1.tar.gz ./uploadToSF/vcf_html_docs.$1.tar.gz
echo Done ! 

tar -cf ./vcf_html_help_docs.$1.tar ./docs/index.chm
tar -rf ./vcf_html_help_docs.$1.tar ./docs/index.chi
tar -rf ./vcf_html_help_docs.$1.tar ./docs/WorkingInVCF.chm
tar -rf ./vcf_html_help_docs.$1.tar ./docs/WorkingInVCF.chi
gzip -f ./vcf_html_help_docs.$1.tar
mv ./vcf_html_help_docs.$1.tar.gz ./uploadToSF/vcf_html_help_docs.$1.tar.gz